#include<iostream>
using namespace std;
int main()
{
    int arr[] = {10, 20, -5, 50, 40, -6, 99, -7, -3, 45, 68, 77};
    int n = sizeof(arr) / sizeof(arr[0]);
    int size = 0;
    int temp[n];
    for(int i = 0; i < n; i++)
    {
        if(arr[i] >= 0)
        {
            temp[size++] = arr[i];
        }
    }
    for(int i = 0; i < n; i++)
    {
        if(arr[i] < 0)
        {
            temp[size++] = arr[i];
        }
    }
    for(int i = 0; i < size; i++)
    {
        cout<<temp[i]<<" ";
    }
}
