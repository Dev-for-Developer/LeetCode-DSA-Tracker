#include<iostream>
using namespace std;
int main()
{
    int s; cin>>s;
    int arr[s];
    for(int i = 0; i < s; i++)
    {
        cin>>arr[i];
    }
    for(int i = 0; i < s; i++)
    {
        int sum = 0;
        for(int j = 0; j < arr[i]; j++)
        {
            if(j % 5 == 0 || j % 3 == 0)
            {
                sum += j;
            } 
        }
        cout<<sum<<endl;   
    }
}


